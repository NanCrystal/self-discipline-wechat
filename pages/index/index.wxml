<!-- pages/welcome.wxml -->
<view class="welcome-page">
    <view class= class="welcome-container">
        <swiper bindchange="handleChange" class="welcome-swiper" easing-function="{{easing}}" circular="{{circular}}" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
            <block wx:for="{{background}}" wx:key="*this">
                <swiper-item>
                    <image class="swiper-item" src="{{item}}" mode="aspectFill"/>
                </swiper-item>
            </block>
        </swiper>
    </view>
    <view class="welcome-pass"  bindtap="onPass">
        跳过
    </view>
    <view class="welcome-text" animation="{{animationData}}">
        <view class="desc-item">记</view>
        <view class="desc-item">录</view>
        <view class="desc-item">美</view>
        <view class="desc-item">好</view>
        <view class="desc-item">生</view>
        <view class="desc-item">活</view>
          <!-- <image class="welcome-image" src="{{welcome}}"  mode="aspectFit" /> -->
    </view>
</view>